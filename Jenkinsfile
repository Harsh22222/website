pipeline {
    agent any

    stages {
        stage('Building Website') {
            steps {
                sh 'echo "Building Website"'
            }
       }

       stage('Testing Website') {
            steps {
                sh 'echo "Testing Website"'
            }
       }

       stage('Push to Production') {
            steps {
                sh 'echo "Pushing to Production"'
            }
       }
    }
}
